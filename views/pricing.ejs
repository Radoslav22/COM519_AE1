  <body>
    <%- include('common/header'); -%>
  <div class="container">
    
     
            <div class="row justify-content-md-center">
              <div class="alert alert-primary" role="alert">
                    
              </div>
            </div>

      
    
    <div class="row col-md-12 col-md-offset-2 custyle mx-auto">
      
    <button type="button" class="btn btn-primary">Create a new taster</button> 
    <h1 class="mx-auto"> üç∑ Wine Tastings  üç∑  </h1>
  
  
    <table class="table table-striped custab">
    <thead>
        <tr>
            <th>Maker</th>
            <th>Model</th>
            <th>Mileage</th>
            <th>Engine power</th>
            <th>Registration Date</th>
            <th>Type of fuel</th>
            <th>Paint color</th>
            <th>Car type</th>
            <th>Price</th>
            <th>Sold At</th>
        </tr>

        <%  firstResult.forEach(result => { %>

            <tr>
                <td><%=  result.maker_key  %></td>
                <td><%=  result.model_key  %></td>
                <td><%=  result.mileage  %></td>
                <td><%=  result.engine_power  %></td>
                <td><%=  result.registration_date  %></td>
                <td><%=  result.fuel  %></td>
                <td><%=  result.paint_color  %></td>
                <td><%=  result.car_type  %></td>
                <td><%=  result.price  %></td>
                <td><%=  result.sold_at  %></td>
                <td class="text-center"><a class='btn btn-info btn-xs'><span class="glyphicon glyphicon-edit"></span> Edit</a> 
                
            
         <%  
          });
         %>


       
    </thead>
          
    </table>
    </div>
    
    
